<script id="homeNotice-template" type="text/x-handlebars-template">

    {{#each data}}
    <div class="f-list" id="{{id}}" messageid="{{messageId}}">
        <span class="f-n-icon f-bs-s" id="bs_{{id}}"></span>
        <div class="f-t" lang="{{id}}" id="ft_{{id}}">{{{title}}}</div>

