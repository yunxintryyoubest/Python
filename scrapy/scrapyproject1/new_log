    // })();

    //显示验证码
    function showCodeImg(codeImgArea) {
        var randomKey = new Date().getTime();
        $.ajax({
            url: '/get/code?randomKey=' + randomKey,
            type: "POST",
            dataType: 'json',
            success: function (data) {
                if (data.code == '1') {
                    $("." + codeImgArea + " .codeImg").attr("src", data.resp.src);
                    $("."+ codeImgArea +" .randomKey").val(randomKey);
                } else {

                }
            }
        });
    }
</script>
