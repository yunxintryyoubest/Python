<script type="text/x-handlebars-template" id="tpl-top-content-comments">
    {{#each data}}
    <div class="top-item">
        <a class="top-comm-nick" href="/user/{{this.jid}}/submitted/1" target="_blank">{{this.nick}}</a>
        <em class="top-comm-time">{{this.commentTime}}</em>
        <div style="float:right;">
            {{#ifIsVote this.isVote 0}}
            <a class="hot-comment-ding top-comm-operate-pre" lang="{{this.id}}" linkid="{{this.linkId}}" jid="{{this.jid}}" href="javascript:;">顶[{{this.ups}}]</a>
            {{/ifIsVote}}
            {{#ifIsVote this.isVote -1}}
            <a class="top-comm-operate-after" href="javascript:;" lang="{{this.id}}">顶[{{this.ups}}]</a>
            {{/ifIsVote}}
            {{#ifIsVote this.isVote 1}}
            <a class="top-comm-operate-after" href="javascript:;" lang="{{this.id}}">已顶[{{this.ups}}]</a>
            {{/ifIsVote}}
            {{#ifIsVote this.isVote 0}}
            <a class="hot-comment-cai top-comm-operate-pre" lang="{{this.id}}" linkid="{{this.linkId}}" jid="{{this.jid}}" href="javascript:;">踩[{{this.downs}}]</a>
            {{/ifIsVote}}
            {{#ifIsVote this.isVote 1}}
            <a class="top-comm-operate-after" href="javascript:;" lang="{{this.id}}">踩[{{this.downs}}]</a>
            {{/ifIsVote}}
            {{#ifIsVote this.isVote -1}}
            <a class="top-comm-operate-after" href="javascript:;" lang="{{this.id}}">已踩[{{this.downs}}]</a>
            {{/ifIsVote}}
            <a class="top-comm-operate-pre" href="/link/{{this.linkId}}/comments/{{this.id}}" target="_blank">回复</a>
        </div>
        <div class="top-comm-content">
            {{this.content}}
        </div>
        <a class="top-comm-linktitle" href="/link/{{this.linkId}}" target="_blank">{{this.linksTitle}}</a>
    </div>
    {{/each}}
</script>

