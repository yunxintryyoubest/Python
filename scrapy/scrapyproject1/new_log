        <script type="text/javascript">
            function replaceHttps(imgUrl) {
                if( !!imgUrl && imgUrl.indexOf('https') < 0 ) {
                    return imgUrl.indexOf('http') < 0? imgUrl : imgUrl.split("http:")[1];
                } else {
                    return imgUrl
                }
            }
        </script>

        
        
        
        
        
        
        
        
        
        
        

        
        

