<script id="bindTpl1" type="text/template">
    <style>
        .mb{background:url(/images/op.png);position:fixed;top:0;left:0;width:100%;height:100%;z-index:2;}
        .bind-box{width: 346px;height:300px;background-color:white;border: 1px solid #797979;position: absolute;top: 30%;left: 50%;margin-left: -175px;}
        .bind-box .header{height: 30px;line-height: 30px;margin-bottom:30px;background: #e7ecf2;color:#abb6d2;padding: 0px 10px;font-size: 14px;font-weight:bold;}
        .bind-box .header .close-icon{position:absolute;right:8px;top:8px;width:15px;height:15px;background-position: 0 -499px;cursor: pointer}
        .box-bind-mobile{text-align:center;}
