<script type="text/javascript">

    document.domain="chouti.com";

    $(document).ready(function(){

        //如果是移动设备则用JS生成的聊天

        

        
        
        
        
        
        
        
        
        

        

        function dealMessage(itemObj) {
            var content = itemObj;
            if (content.type == 2 || content.content == undefined) {
                return;
            }
            if (content.type == 3) {
                node.chatbox.chatpage.open({
                    hide: true,
                    jid: content.content.user.jid,
                    callback: function () {
                        var groupContent = {};
                        switch (content.content.type) {
                            case 0:
                                groupContent = {
                                    text: content.content.content.text
                                }
                                break;
                            case 1:
                                groupContent = {
                                    url: content.content.content.url,
                                    height: content.content.content.height,
                                    width: content.content.content.width,
                                    filePath: content.content.content.filePath
                                }
                                break;
                            case 2:
                                groupContent = {
                                    url: content.content.content.url,
                                    duration: Math.floor(content.content.content.duration / 1000),
                                    filePath: content.content.content.filePath
                                }
                                break;
                            case 3:
                                groupContent = {
                                    location: content.content.content.location,
                                    lat: content.content.content.lat,
                                    lon: content.content.content.lon
                                }
                                break;
                            case 7:
                                groupContent = {
                                    text: content.content.content.text
                                }
                                break;
                        }
                        node.chatbox.message.add({
                            tar: $('.page-chat[tar="' + content.content.user.jid + '"]').find('.page-main-content'),
                            status: 'private',
                            content: {
                                content: groupContent,
                                createTime: content.content.createTime,
                                user: {
                                    img_url: replaceHttps(content.content.user.img_url),
                                    nick: content.content.user.nick,
                                    jid: content.content.user.jid
                                },
                                type: content.content.type,
                                id: content.content.id
                            },
                            isnew: true
                        });
                    }
                });
            } else {
                node.chatbox.message.add({
                    tar: $('.chat-public-content'),
                    status: null,
                    content: {
                        content: {
                            text: content.content.content.text
                        },
                        createTime: content.content.createTime,
                        user: {
                            img_url: replaceHttps(content.content.user.img_url),
                            nick: content.content.user.nick,
                            jid: content.content.user.jid
                        },
                        type: 0,
                        id: content.content.id
                    },
                    isnew: true
                });
            }
        }

        //订阅用户未读通知数的通知
        gpComet = new GozapComet({
            serviceUrl:'//comet.chouti.com/comet',
            iframeUrl:'//comet.chouti.com/update.jsp',
            appId:"chouti-new"
        });
        gpComet.broadcast(function(message){

            var itemObj = eval("(" + message.data + ")");

            var newsCount = parseInt(itemObj.freshNewsCount,10);
            if(newsCount <= 0){
                return;
            }

            if( itemObj.cometType && itemObj.cometType == 1 ) {
                //执行添加消息的逻辑
                if( itemObj.content.user.jid != node.chatbox.user.jid ) {
//                                node.chatbox.message.add({
//                                        tar: $('.chat-public-content'),
//                                        status: null,
//                                        content: itemObj.content,
//                                        isnew: true
//                                });

                    dealMessage(itemObj);
                }

            } else {
                var tempCount = parseInt($("#newIntoHotCount").val(),10);
                tempCount = tempCount + newsCount;
                $("#newIntoHotCount").val(tempCount);

                $("#yellow-msg-box-intohot em").text(tempCount+"");
                $("#yellow-msg-box-intohot").show().unbind();
                //新榜入热榜黄色提示框事件
                NS_links_hotts.yellowIntohotBoxHover();
            }



        });
        gpComet.deliver(function(message){
            var itemObj = eval("(" + message.data + ")");

            if(itemObj.length <= 0) {
                return;
            }

            if( itemObj.cometType && itemObj.cometType == 2 ) {
                //执行添加私聊消息的逻辑
                if( itemObj.content.user.jid != node.chatbox.user.jid ) {

//                                node.chatbox.message.add({
//                                        tar: $('.page-chat[tar="' + itemObj.content.user.jid + '"]').find('.page-main-content'),
//                                        status: 'private',
//                                        content: itemObj.content,
//                                        isinit: true
//                                });

                    dealMessage(itemObj);
                }
            } else {
                //改变未读通知数
                var newsCount = itemObj.notifyCount;

                if(newsCount != undefined){

                    var newsCount2 = parseInt(itemObj.notifyCount);

                    //当为0时隐藏提示
                    if(newsCount2 <=0) {

                        $("#notice-num-title").hide().children("em").text("");
                        return;
                    }

                    chouti.chgNoticeNum(newsCount2);

                }


                //改变未读动态数
                var DtaiCount = itemObj.flowState;
                if(DtaiCount != undefined){

                    var DtaiCount2 = parseInt(itemObj.flowState);
                    //当为0时隐藏提示

                    if(DtaiCount2 <=0) {
                        $("#Dtai-num-title").hide().children("em").text("");
                        return;
                    }
                    chouti.chgDtaiNum(DtaiCount2);
                }
            }



        });

        //以下代码实现的功能为：
        //在新热榜登录成功后，可点击 "我的抽屉”进入后，相应的也是登录状态。

        // puid = $.cookie("puid");

        // $("#cookieIframe").attr("src", "http://www.chouti.com/cookieService?d="+puid);

        chouti.showStreamNotice(); //触发显示通知数

        //chouti.showStreamMail(); //触发显示私信数

        chouti.showStreamDtai(); //触发显示动态数

        chouti.showUserOprBox(); //显示登录用户操作框（点击头部用户昵时）

        chouti.showHomePageNotice(); //点击通知链接显示首页通知框

        chouti.logout(); //退出登录

        chouti.showGoTop(); //回到页面顶部

        var userAgentInfo2=navigator.userAgent;


        if(userAgentInfo2.indexOf("Android") > 0 || userAgentInfo2.indexOf("iPhone") > 0 ||
                userAgentInfo2.indexOf("iPad") > 0 || userAgentInfo2.indexOf("iPod") > 0){

        } else {
            chouti.fixedAdvert(); //固定广告的位置
        }

        //chouti.playVido(); //展开视频
    })



    // var _paq = _paq || [];
    // /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
    // _paq.push(['trackPageView']);
    // _paq.push(['enableLinkTracking']);
    // (function() {
    //     var u="//collect.chouti.com/";
    //     _paq.push(['setTrackerUrl', u+'piwik.php']);
    //     _paq.push(['setSiteId', '1']);
    //     var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    //     g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'piwik.js'; s.parentNode.insertBefore(g,s);
    // })();

    //显示验证码
    function showCodeImg(codeImgArea) {
        var randomKey = new Date().getTime();
        $.ajax({
            url: '/get/code?randomKey=' + randomKey,
            type: "POST",
            dataType: 'json',
            success: function (data) {
                if (data.code == '1') {
                    $("." + codeImgArea + " .codeImg").attr("src", data.resp.src);
                    $("."+ codeImgArea +" .randomKey").val(randomKey);
                } else {

                }
            }
        });
    }
</script>
<!-- 当操作平台为移动设备，如手机，ipad时，引入JS文件，用作flash聊天工具 -->




	</div>
</div>
