<!-- 举报弹出框（举报原因） -->
<script type="text/x-handlebars-template" id="jubao-dialog-code">

    <div id="Jubao-dialog" class="mail-dialog dialog corner" onClick='function A(e){if (!e) var e = window.event;e.cancelBubble = true;if (e.stopPropagation) e.stopPropagation();};A();'>
        <div class="dialog-titlebar">
            <div class="dialog-title" id="jubaoTitle">
                <span>举报</span>
            </div>
            <div class="dialog-titlebar-close" id="jbTClose"><a id="dialog-jubao-close" class="icons dialog-btn-close"
                                                                title="关闭" href="javascript:;"></a></div>
        </div>
        <div class="m-dialog-content dialog-content corner-bottom" id="JubaoBox">

            <div id="dialog-main-content" class="dialog-main-content">
                <div class="mail-area" style="margin-bottom:0px;">

